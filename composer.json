{
    "name" : "jsimo/laravel-repository-pattern",
    "type" : "library",
    "description" : "A Laravel Repository - Pattern",
    "keywords": [
        "laravel",
        "laravel repository",
        "laravel repository pattern",
        "laravel service",
        "repository service",
        "service",
        "repository",
        "base repository",
        "laravel service and repository",
        "service and repository",
        "repository",
        "eloquent",
        "model",
        "cache",
        "code generator",
        "laravel skeleton",
        "laravel boilerplate"
    ],
    "homepage" : "https://github.com/Jsimo/laravel-repository-pattern",
    "support" : {
        "issues" : "https://github.com/Jsimo/laravel-repository-pattern/issues"
    },
    "authors" : [
        {
            "name" : "Simo Joel",
            "email" : "joelsimooverride@gmail.com",
            "role": "Developper",
            "homepage": "https://xpeedy.com"
        }
    ],
    "license" : "MIT",
    "require" : {
        "php": "^7.3|^8",
        "ext-json": "*",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "laravel/framework": "^6.0|^7.0|^8.74|^9.0|^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
